<p>
  <label for="email">Email<span class="required"> *</span></label>
  <%= f.text_field :email %>
  <em></em>
</p>

<p>
  <label for="password">Пароль<span class="required"> *</span></label>
  <%= f.text_field :password %>
  <em></em>
</p>

<p>
  <label for="password_confirmation">Подтвердить Пароль<span class="required"> *</span></label>
  <%= f.text_field :password_confirmation %>
  <em></em>
</p>

<p>
  <label for="password_confirmation">Привязка<span class="required"> *</span></label>
  <%= f.text_field :parent_id %>
  <em>Указать номер связи с пользователем</em>
</p>

<p>
  <label for="password_confirmation">И.Ф.О<span class="required"> *</span></label>
  <%= f.text_field :full_name %>
  <em></em>
</p>

<p>
  <label for="password_confirmation">Активность<span class="required"> *</span></label>
  <%= f.check_box :active %>
  <em></em>
</p>

